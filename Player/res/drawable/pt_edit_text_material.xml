<?xml version="1.0" encoding="utf-8" ?>
<inset xmlns:android="http://schemas.android.com/apk/res/android">
	<selector>
		<item android:state_enabled="false">
			<nine-patch android:src="@drawable/pt_textfield_nomal_mtrl_light" android:tint="?colorControlNormal" android:alpha="?android:disabledAlpha" />
		</item>
		<item android:state_focused="false" android:state_pressed="false">
			<nine-patch android:src="@drawable/pt_textfield_nomal_mtrl_light" android:tint="?colorControlNormal" />
		</item>
		<item>
			<nine-patch android:src="@drawable/pt_textfield_nomal_mtrl_light" android:tint="?colorControlActivated" />
		</item>
	</selector>
</inset>
