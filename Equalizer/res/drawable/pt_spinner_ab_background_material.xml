<?xml version="1.0" encoding="utf-8" ?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?android:colorControlActivated">
	<item>
		<selector>
			<item android:state_pressed="true">
				<nine-patch android:src="@drawable/pt_spinner_ab_pressed_mtrl_light" />
			</item>
			<item android:state_focused="true">
				<nine-patch android:src="@drawable/pt_spinner_ab_focused_mtrl_light" />
			</item>
			<item android:state_enabled="false">
				<nine-patch android:src="@drawable/pt_spinner_ab_disabled_mtrl_light" />
			</item>
			<item>
				<nine-patch android:src="@drawable/pt_spinner_ab_default_mtrl_light" />
			</item>
		</selector>
	</item>
</ripple>
